<h1> Home </h1>
<p>This is the {{ title }} page</p>
{{#if isAvailable}}
<ul>
    {{#each rooms}}
        <li>
            {{this.name}} <a href={{this.name}}></a>
        </li>
    {{/each}}
</ul>
{{/if}}

<form action="/create" method="POST">
<input type="text" name="roomname" placeholder="Please enter Room Name">
<input type="submit" value="Create room">
</form>
